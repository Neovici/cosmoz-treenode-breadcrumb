{
  "name": "cosmoz-treenode-breadcrumb",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/Neovici/cosmoz-treenode-breadcrumb.svg?branch=master)](https://travis-ci.org/Neovici/cosmoz-treenode-breadcrumb)",
  "main": "cosmoz-treenode-breadcrumb.html",
  "directories": {
    "test": "test"
  },
	"devDependencies": {
		"eslint": "^3.9.1",
		"eslint-plugin-html": "^3.2.2",
		"eslint-plugin-mocha": "^4.11.0",
		"polymer-cli": "^1.8.0"
	},
	"scripts": {
		"start": "polymer serve -o",
		"preinstall": "rm -f package-lock.json",
		"postinstall": "polymer install --variants && yarn sync-npm",
		"lint": "eslint --cache --ext .js,.html . && polymer lint cosmoz-*.html",
		"test": "polymer test",
		"analyze": "polymer analyze --input cosmoz-*.html  > analysis.json",
		"sync-npm": "npm shrinkwrap && mv npm-shrinkwrap.json package-lock.json"
	},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neovici/cosmoz-treenode-breadcrumb.git"
  },
  "keywords": [
    "polymer",
    "web-components"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/neovici/cosmoz-treenode-breadcrumb/issues"
  },
  "homepage": "https://github.com/neovici/cosmoz-treenode-breadcrumb#readme"
}
